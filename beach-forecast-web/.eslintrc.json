{
    "env": {
        "browser": true,
        "es2022": true
    },
    "extends": [
        "plugin:@typescript-eslint/recommended",
        "next",
        "next/core-web-vitals",
        "prettier",
        "plugin:@tanstack/eslint-plugin-query/recommended"
    ],
    "plugins": [
        "@typescript-eslint",
        "prettier",
        "@tanstack/query"
    ],
    "parser": "@typescript-eslint/parser",
    "rules": {
        "prettier/prettier": "error",
        "@typescript-eslint/no-explicit-any": "off"
    }
}
